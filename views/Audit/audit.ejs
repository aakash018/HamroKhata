<h2 class="page-header">Audit</h2>
<div class="parentTable">
    <div class="Column">
        <h3>Aakash</h3>
        <div class="column_data">
            <% let i = 0 %>
            <% entries.forEach(entry => { %>
            <div><% if (entry.person == "Aakash" && i < 10) { %>
                <% i += 1 %>
                <%= entry.amount %>
                <% console.log(i); %>
                <% } %>
            </div>
            <%}) %>
        </div>
        <div class="calc_table">
            <div>
                Subash :
                <%= audites[audites.length - 1].Subash_Aakash %>
            </div>
            <div>
                Yaman :
                <%= audites[audites.length - 1].Yaman_Aakash %>
            </div>
        </div>
    </div>
    <hr>
    <div class="Column">
        <h3>Subash</h3>
        <div class="column_data">
            <% let j = 0 %>
            <% entries.forEach(entry => { %>
            <div><% if (entry.person == "Subash" && j < 10) { %>
                <% j += 1 %>
                <%= entry.amount %>
                <% } %>
            </div>
            <%}) %>
            <div class="calc_table">
                <div>
                    Aakash :
                    <%= audites[audites.length - 1].Aakash_Subash %>
                </div>
                <div>
                    Yaman :
                    <%= audites[audites.length - 1].Yaman_Subash %>
                </div>
            </div>
        </div>
    </div>
    <hr>
    <div class="Column">
        <h3>Yaman</h3>
        <div class="column_data">
            <% let k = 0 %>
            <% entries.forEach(entry => { %>
            <div><% if (entry.person == "Yaman" && k < 10 )  { %>
                <% k += 1 %>
                <%= entry.amount %>
                <% } %>
            </div>
            <%}) %>
            <div class="calc_table">
                <div>
                    Aakash :
                    <%= audites[audites.length - 1].Aakash_Yaman %>
                    </>
                    <div>
                        Subash :
                        <%= audites[audites.length - 1].Subash_Yaman %>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>